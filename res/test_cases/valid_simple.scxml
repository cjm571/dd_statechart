<?xml version="1.0"?>
<scxml xmlns="http://www.w3.org/2005/07/scxml"
       name="raise_verf"
       version="1.0"
       datamodel="ecmascript"
       initial="off">

  <state id="off">
    <transition event="turn.on" target="on"/>
  </state>

  <state id="on">
    <transition event="turn.off" target="off"/>
  </state>
</scxml>